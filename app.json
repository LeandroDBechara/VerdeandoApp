{
  "expo": {
    "name": "verdeandoApp",
    "slug": "verdeandoapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/dodo-games-logo.png",
    "scheme": ["verdeandoapp","fb674819048296621"],
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "bundleIdentifier": "com.verdeando.app",
      "supportsTablet": true
    },
    "android": {
      "package": "com.verdeando.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/dodo-games-logo.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/dodo-games-logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "051543c8-14cd-469e-8097-21ca1e27d526"
      }
    },
    "owner": "leodbechara"
  }
}
