{
  "expo": {
    "name": "verdeandoApp",
    "slug": "verdeandoapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/dodo-games-logo.png",
    "scheme": [
      "verdeandoapp",
      "fb674819048296621"
    ],
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "bundleIdentifier": "com.verdeando.app",
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Esta app necesita acceso a tu ubicación para mostrar puntos verdes cercanos en el mapa.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "Esta app necesita acceso a tu ubicación para mostrar puntos verdes cercanos en el mapa.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "config": {
        "googleMapsApiKey": "process.env.EXPO_PUBLIC_GOOGLE_MAPS_API_KEY"
      }
    },
    "android": {
      "package": "com.verdeando.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/dodo-games-logo.png"
      },
      "config": {
        "googleMaps": {
          "apiKey": "process.env.EXPO_PUBLIC_GOOGLE_MAPS_API_KEY"
        }
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/dodo-games-logo.png"
    },
    "plugins": [
      "expo-router",
      
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Esta app necesita acceso a tu ubicación para mostrar puntos verdes cercanos en el mapa."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Esta app necesita acceso a tus fotos para permitirte compartirlas con tus amigos."
        }
      ],
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "owner": "leodbechara",
    "extra": {
      "router": {},
      "eas": {
        "projectId": "ea866c7a-a28b-415c-99b3-1f86c08a407a"
      }
    }
  }
}
